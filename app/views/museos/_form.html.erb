<%=form_for @museo do |f|%>
    <%=f.label :nombre,:class=>'label'%>
    <%=f.text_field :nombre%>
    <br/>
    <%=f.label :nombre_corto,:class=>'label'%>
    <%=f.text_field :nombre_corto%>
    <br/>
  
    <br/>
	
    <%=f.submit 'Guardar',:class=>'btn btn-success'%> o <%=link_to 'cancelar',museos_path%>
<%end%>

